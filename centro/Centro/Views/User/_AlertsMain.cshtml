@model List<Alert>
@if (Model.Count() > 0)
{
<div class="activity-log-pop">
    <ul>
    @foreach (var item in Model)
    {
        
        <li>
            <a href="@item.AlertLink">
                <strong>@Html.Raw(item.AlertText)</strong>
                <span class="date grey">@item.CreatedOn.ToShortDateString() @item.CreatedOn.ToShortTimeString()</span>
            </a>
        </li>
    }
    </ul>
</div>
}