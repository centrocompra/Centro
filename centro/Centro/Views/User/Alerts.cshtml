@model PagingResult<Alert>
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_BuyerLayout.cshtml";
}
@section title{
    User Alerts
}
@{
    
}
<div class="white-area-inner">
    @Html.Partial("_BuyerLeftSideMenu")
    <div class="profile-right-bar primary">
        <div class="primary">
			<div class="row">
				<h2>User Alerts</h2>
			</div>
			<div class="activity-log-mn">
                @Html.Partial("_Paging")
				<div class="activity-log-mn-tbl row" id="Result" >
                    @Html.Partial("_AlertsMain", Model.List)
				</div>
                @Html.Partial("_Paging")
			</div>
		</div>
    </div>
</div>
@section scripts{
<script type="text/javascript">
    var totalCount = 0;
    $(document).ready(function () {
        totalCount = parseInt('@Model.TotalCount');               
    });
</script>
<link href="@Url.Content("~/Content/paging/style.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/Custom/paging.js")" type="text/javascript"></script>    
<script src="@Url.Content("~/Scripts/Custom/AlertsPaging.js")" type="text/javascript"></script>
}
